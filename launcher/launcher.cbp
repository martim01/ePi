<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="launcher" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="../bin/launcher" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="/home/pi/ePi/bin/launch.ini" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library="cap" />
					<Add library="pml_logd" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="../bin/launcher" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="/home/pi/ePi/bin/cart.ini 1" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="cap" />
					<Add library="pml_log" />
				</Linker>
				<ExtraCommands>
					<Add after="sudo setcap cap_sys_admin+ep /home/pi/ePi/bin/launcher" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-std=c++14" />
			<Add option="-fexceptions" />
			<Add directory="include" />
			<Add directory="../common/include" />
			<Add directory="$(#json.INCLUDE)" />
			<Add directory="$(#log.INCLUDE)" />
		</Compiler>
		<Linker>
			<Add directory="$(#log.LIB)" />
			<Add directory="../lib" />
		</Linker>
		<Unit filename="../common/include/inimanager.h" />
		<Unit filename="../common/include/inisection.h" />
		<Unit filename="../common/src/epiutils.cpp" />
		<Unit filename="../common/src/inimanager.cpp" />
		<Unit filename="../common/src/inisection.cpp" />
		<Unit filename="include/launcher.h" />
		<Unit filename="src/launcher.cpp" />
		<Unit filename="src/main.cpp" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
